{
  "name": "jpg-to-webp-landing",
  "version": "1.0.0",
  "description": "Landing page for JPG to WebP Chrome extension",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000 --no-browser",
    "dev": "live-server --port=3000",
    "build": "mkdir -p dist && cp -r *.html *.css images favicons dist/",
    "deploy": "npm run build && gh-pages -d dist",
    "preview": "cd dist && live-server --port=3001"
  },
  "keywords": ["chrome extension", "webp", "jpg", "landing page"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "gh-pages": "^6.0.0"
  }
} 